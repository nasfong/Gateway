{
  "version": 2,
  "regions": [
    "hkg1"
  ],
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/dist/server.js",
      "dest": "/dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.ts"
    }
  ]
}